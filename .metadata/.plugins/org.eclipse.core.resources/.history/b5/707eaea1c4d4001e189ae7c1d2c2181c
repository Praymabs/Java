package Day4;

public class LargestOf3Number {

	public static void main(String[] args) 
	{
		
	int a=10, b=20, c=30;
	if(a>b)
	{
		if(a>c)
		{
			System.out.println(a);
		}
		else
		{
			System.out.println(c);
		}
	}
	
	else if(a < b)
	{
		if(b>c)
		{
			System.out.println(b);
		}
		else
		{
			System.out.println(c);
		}
	}
	
	//AnotherWay
	int x=25,y=15, z=5;
	if (x>y && x>z)
	{
		System.out.println(x);
	}
	else if(y>x && y>z)
	{
		System.out.println(y);
	}
	else
	{
		System.out.println(z);
	}
		

	}

}
