package Day4;

public class LargestOf3Number {

	public static void main(String[] args) 
	{
		
	int a=10, b=20, c=30;
	if(a>b)
	{
		if(a>c)
		{
			System.out.println(a);
		}
		else
		{
			System.out.println(c);
		}
	}
	
	if(a < b)
	{
		if(b>c)
		{
			System.out.println(b);
		}
		else
		{
			System.out.println(c);
		}
	}

	}

}
